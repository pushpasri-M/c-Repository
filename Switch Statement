using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class LearningHowToProgram : MonoBehaviour
{
   private void Start() {
        float health = 100;
        // Switch Statement
        switch (health)
        {
            case 100:
                Debug.Log("Your HP is High");
                break;
            case 50:
                Debug.Log("Your HP is High");
                break;
            default:
                Debug.Log("you HP is invalied");
                break;
        }
    }
    
}
