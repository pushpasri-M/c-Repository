using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class LearningHowToProgram : MonoBehaviour
{
    private void Start()
    {
        int loop = 6;
        int i;
        // for loop
        for (i = 0; i < loop; i++)
        {
            Debug.Log("the value of i:" + i);
        }
    }

        
}
